filetype on
filetype plugin on
filetype indent on
set autoindent
set wrap
set showcmd
syntax on
set shiftwidth=2
set wildmode=list:longest,full
set backspace=2
colorscheme molokai
set number

set runtimepath^=~/.vim/bundle/ctrlp.vim
nnoremap <C-J> <C-W><C-J> 
nnoremap <C-K> <C-W><C-K>
nnoremap <C-L> <C-W><C-L>
nnoremap <C-H> <C-W><C-H>


